@model VisualizeTableDto

<form method="post" asp-action="Save">
    <div class="form-group">
        <label asp-for="ApiField">API Field</label>
        <div class="form-row">
            <div class="col-5">
                <select class="form-control" asp-for="ApiField" asp-items="@((List<SelectListItem>) ViewData["Items"])"></select>
            </div>
        </div>
    </div>
    <div class="form-group">
        <label asp-for="StartDate">Date Range</label>
        <div class="form-row">
            <div class="col-6">
                <input type="datetime-local" class="form-control" asp-for="StartDate">
            </div>
            <div class="col-6">
                <input type="datetime-local" class="form-control" asp-for="EndDate">
            </div>
        </div>
    </div>

    <button type="submit" class="btn btn-primary">Show</button>
</form>